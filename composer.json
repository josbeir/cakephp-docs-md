{
    "name": "josbeir/cakephp-docs-md",
    "description": "CakePHP markdown docs converter and generator",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cake\\DocsMD\\": "converter/src/"
        }
    },
    "authors": [
        {
            "name": "Jasper Smet",
            "email": "josbeir@gmail.com"
        }
    ],
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.2",
        "rector/rector": "^2.1"
    },
    "scripts": {
        "test": "php converter/test/run-tests.php",
        "cs-check": "phpcs --colors -p -s",
        "cs-fix": "phpcbf --colors -p -s",
        "convert-en": "php scripts/convert.php ./legacy/en ./docs/en",
        "rector": "rector --ansi"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
